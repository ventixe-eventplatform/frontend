{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/favicon.ico",
      "/images/*",
      "/css/*",
      "/js/*"
    ]
  },
  "routes": [
    {
      "route": "/auth/*",
      "method": "POST",
      "forward": "https://ventixe-app-authservice.azurewebsites.net/api/auth/*"
    },
    {
      "route": "/auth/*",
      "method": "OPTIONS",
      "forward": "https://ventixe-app-authservice.azurewebsites.net/api/auth/*"
    },
    {
      "route": "/accounts/*",
      "method": "POST",
      "forward": "https://ventixe-app-accountservice.azurewebsites.net/api/accounts/*",
    }
  ]
}
