{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/favicon.ico",
      "/images/*",
      "/css/*",
      "/js/*"
    ]
  },
  "routes": [
    {
      "route": "/auth/*",
      "rewrite": "https://ventixe-app-authservice.azurewebsites.net/api/auth/:*",
      "allowedRoles": ["anonymous"]
    },
        {
      "route": "/accounts/*",
      "rewrite": "https://ventixe-app-accountservice.azurewebsites.net/api/accounts/:*",
      "allowedRoles": ["anonymous"]
    },
      {
      "route": "/accountProfiles/*",
      "rewrite": "https://ventixe-app-accountprofileservice.azurewebsites.net/api/accountProfiles/:*",
      "allowedRoles": ["anonymous"]
    }
  ]
}
